{
  "name": "solution-configuration-management-service",
  "version": "1.0.0",
  "description": "solution-configuration-management-service",
  "main": "index.js",
  "scripts": {
    "start:dev": "node --env-file=.env.test dist/main/server.js",
    "build": "npm run clean  && tsc --sourceMap --p tsconfig.build.json && tsc-alias",
    "test": "jest --passWithNoTests --no-cache --runInBand --env-file=.env.test",
    "test:unit": "npm test -- --watch -c jest-unit.config.js",
    "test:integration": "npm test -- --watch -c jest-integration.config.js",
    "test:staged": "npm test --findRelatedTests",
    "test:watch": "npm test -- --watch --no-cache",
    "test:ci": "npm test -- --coverage",
    "clean": "rimraf dist/",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DiegoSalas27/solution-configuration-management-service.git"
  },
  "keywords": [],
  "author": "",
