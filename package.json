  "bugs": {
    "url": "https://github.com/DiegoSalas27/solution-configuration-management-service/issues"
  },
  "homepage": "https://github.com/DiegoSalas27/solution-configuration-management-service#readme",
  "devDependencies": {
    "@faker-js/faker": "^6.0.0-alpha.7",
    "@types/express": "^5.0.3",
    "@types/jest": "^27.4.1",
    "@types/node": "^20.10.0",
    "eslint-config-standard-with-typescript": "^40.0.0",
    "husky": "^8.0.0",
    "jest": "^27.5.1",
    "jest-mock-extended": "^3.0.1",
    "lint-staged": "^15.1.0",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "ts-jest": "^27.1.3",
    "tsc-alias": "^1.8.16"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.68.0",
    "coffeeless-schema-validators": "^2.12.1",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "openai": "^6.7.0",
    "sqlite3": "^5.1.7"
  }
}
